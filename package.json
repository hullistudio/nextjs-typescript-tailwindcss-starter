{
  "name": "nextjs-typescript-tailwindcss-starter",
  "description": "Starter Boilerplate for Next.js, Tailwind CSS, TypeScript and React.js",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Hulli Studio",
    "email": "contact@hulli.studio",
    "url": "https://hulli.studio"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
